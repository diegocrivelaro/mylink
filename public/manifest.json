{
  "name": "MyLink",
  "short_name": "MyLink",
  "description": "MyLink is a link shortener that allows you to shorten any link you want.",
  "author": "Diego Crivelaro",
  "version": "1.0.0",
  "manifest version": 3,
  "default locale": "pt-BR",
  "display": "standalone",
  "start_url": ".",
  "theme_color": "#282a36",
  "background_color": "#282a36",
  "icons": [
    {
      "src": "favicon.ico",
      "sizes": "64x64 32x32 24x24 16x16",
      "type": "image/x-icon"
    },
    {
      "src": "logo192.png",
      "type": "image/png",
      "sizes": "192x192"
    },
    {
      "src": "logo512.png",
      "type": "image/png",
      "sizes": "512x512"
    }
  ]
}
